[eval exp="tf.size='defo'"]
[eval exp="tf.gra_kuroda_pose = '[黒田_ベース羽織]'"]
[chara_mod name=kuroda_base storage="kuroda/base_haori.png" time=0]
[wait time=10]
[anim name=kuroda_base width=513 time=10]
[endmacro]
